# Ignore bundler config.
/.bundle

# Ignore RubyMine files
.idea/*

# Ignore the default SQLite database.
/db/*.sqlite3
/db/*.sqlite3-journal


# Ignore all logfiles and tempfiles.
/log/*.log
/tmp

# Ignore jetty server, solr, fedora, associated data directories
jetty/*

# Ignore fits
fits/*

# Ignore the database and pul_store configs
database.yml
pul_store.yml

noid-minter-state

# ignore compiled assets and bundled gems
public/assets
vendor/bundle

.DS_Store
